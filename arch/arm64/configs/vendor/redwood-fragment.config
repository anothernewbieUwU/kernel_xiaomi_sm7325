# Redwood Config
CONFIG_MACH_XIAOMI_REDWOOD=y

# Charger Configs
CONFIG_AI_RSOC=y
CONFIG_BQ_CLOUD_AUTHENTICATION=y
CONFIG_BQ_FG_1S=y
CONFIG_BQ_FUEL_GAUGE=y
CONFIG_BQ2597X_BATTERY_CHARGER=y

# Finger print Configs
CONFIG_FINGERPRINT_GOODIX_TEE=y
CONFIG_FINGERPRINT_XIAOMI_FP=y

# Haptics Config
CONFIG_INPUT_AW86927_HAPTIC=y

# Disable mmc driver
# CONFIG_MMC is not set

# Disable fsa4480 (Redwood doesn't support usb audio)
# CONFIG_QCOM_FSA4480_I2C is not set

# KernelSU
CONFIG_KSU=y
CONFIG_KSU_SUSFS=y
CONFIG_KSU_SUSFS=y
CONFIG_KSU_SUSFS_HAS_MAGIC_MOUNT=y
CONFIG_KSU_SUSFS_SUS_PATH=y
CONFIG_KSU_SUSFS_SUS_MOUNT=y
CONFIG_KSU_SUSFS_AUTO_ADD_SUS_KSU_DEFAULT_MOUNT=y
CONFIG_KSU_SUSFS_AUTO_ADD_SUS_BIND_MOUNT=y
CONFIG_KSU_SUSFS_SUS_KSTAT=y
CONFIG_KSU_SUSFS_SUS_OVERLAYFS=y
CONFIG_KSU_SUSFS_TRY_UMOUNT=y
CONFIG_KSU_SUSFS_AUTO_ADD_TRY_UMOUNT_FOR_BIND_MOUNT=y
CONFIG_KSU_SUSFS_SPOOF_UNAME=y
# CONFIG_KSU_SUSFS_ENABLE_LOG is not set
CONFIG_KSU_SUSFS_HIDE_KSU_SUSFS_SYMBOLS=y
CONFIG_KSU_SUSFS_SPOOF_CMDLINE_OR_BOOTCONFIG=y
CONFIG_KSU_SUSFS_OPEN_REDIRECT=y

# MGLRU
CONFIG_LRU_GEN=y
CONFIG_LRU_GEN_ENABLED=y